{
  "presets": [
    ["@babel/preset-env", {
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": ["transform-vue-jsx", "@babel/plugin-transform-runtime","@babel/plugin-syntax-dynamic-import"],
  "env": {
    "test": {
      "presets": ["env"],
      "plugins": ["transform-vue-jsx", "transform-es2015-modules-commonjs", "dynamic-import-node"]
    }
  }
}
